<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="demo_draft" model="eth.state">
            <field name="name">Draft</field>
            <field name="index">0</field>
        </record>
        <record id="demo_approve" model="eth.state">
            <field name="name">Approve</field>
            <field name="index">1</field>
        </record>
        <record id="demo_confirmed" model="eth.state">
            <field name="name">Confirmed</field>
            <field name="index">2</field>
        </record>
        <record id="demo_cancel" model="eth.state">
            <field name="name">Cancel</field>
            <field name="index">3</field>
        </record>
        <record id="demo_draft_approve" model="eth.state.eth.state.rel">
            <field name="state_first_id" ref="demo_draft" />
            <field name="state_second_id" ref="demo_approve" />
        </record>
        <record id="demo_approve_confirmed" model="eth.state.eth.state.rel">
            <field name="state_first_id" ref="demo_approve" />
            <field name="state_second_id" ref="demo_confirmed" />
        </record>
        <record id="demo_approve_cancel" model="eth.state.eth.state.rel">
            <field name="state_first_id" ref="demo_approve" />
            <field name="state_second_id" ref="demo_cancel" />
        </record>
        <record id="demo_cancel_draft" model="eth.state.eth.state.rel">
            <field name="state_first_id" ref="demo_cancel" />
            <field name="state_second_id" ref="demo_draft" />
        </record>
        <record id="demo_basic_contract" model="eth.contract">
            <field name="name">Basic State machine Contract</field>
            <field name="state_id" ref="demo_draft" />
        </record>
    </data>
</odoo>
